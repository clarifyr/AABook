bikes$start.coord <- paste(bikes$start.station.latitude, bikes$start.station.longitude, sep = ", ") ##combines starting lat/lon
bikes$end.coord <- paste(bikes$end.station.latitude, bikes$end.station.longitude, sep = ", ") ##combines ending lat/lon
routecount <- count(bikes, c("start.coord", "end.coord")) ##gives count for each route
ordered.routes <- routecount[order(-routecount$freq),] ##orders based on frequency
route_df <- route(ordered.routes$start.coord[3], ordered.routes$end.coord[3], mode = "bicycling", structure = "route") ##route function
